<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.comtop.cap.runtime.component.model">
	<select id="loadBizSequenceByKey" resultType="com.comtop.cap.runtime.component.model.BizSequenceVO">
		SELECT  * FROM CAP_RT_BIZ_SEQUENCE 
		WHERE SEQ_KEY = #{key}
